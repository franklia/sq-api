{
  "name": "api",
  "version": "1.0.0",
  "description": "API for Spot Quiz app",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && ./node_modules/.bin/babel src --out-dir dist",
    "clean": "rimraf dist",
    "predeploy": "npm run build",
    "deploy": "eb deploy",
    "postdeploy": "npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "dev": "nodemon --exec babel-node src/server.js"
  },
  "author": "Frank Liardet",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "4.17.1",
    "mongoose": "^5.4.1",
    "npm": "6.14.6",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.9.6",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-register": "^6.26.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "nodemon": "^1.18.9",
    "prettier": "2.0.5"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 140
  }
}
